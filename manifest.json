{
	"manifest_version" : 2,
	"name" : "Dr.URL",
	"version" : "1.0",
	"description" : "Bookmark manager",
	"background" : {
		"persistent" : false,
		"scripts" : ["popup.js"]
	},
	"permissions" : [
		"bookmarks"
	],
	
	"browser_action" : {
		"default_icon" : "icon.png",
		"default_popup" : "popup.html"
	},
	"content_security_policy" : "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}
